<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="250.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <StackPane fx:id="allContentArea" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox prefHeight="35.0" prefWidth="1000.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="33.0" prefWidth="233.0" text="Attendance detail for date: " textFill="#3d5c98">
                           <font>
                              <Font name="Arial Bold" size="13.0" />
                           </font>
                        </Label>
                        <Label fx:id="dateValue" prefHeight="36.0" prefWidth="88.0" textFill="#3d5c98">
                           <font>
                              <Font name="Arial Bold" size="13.0" />
                           </font>
                        </Label>
                     </children>
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </HBox>
                  <TableView fx:id="employeeAttendanceDetaillTable" prefHeight="200.0" prefWidth="1000.0" stylesheets="@../../../../../../Downloads/table.css" />
               </children>
            </VBox>
         </children>
      </StackPane>
   </center>
</BorderPane>
