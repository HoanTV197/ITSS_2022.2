                  <HBox prefHeight="38.0" prefWidth="900.0" style="-fx-background-color: #b1ccc8;">
                     <children>
                        <Button fx:id="myAttendanceTab" mnemonicParsing="false" prefHeight="41.0" prefWidth="153.0" styleClass="dashboardBtn" stylesheets="@../../../../../../Desktop/styleDashboard.css" text="My Attendance">
