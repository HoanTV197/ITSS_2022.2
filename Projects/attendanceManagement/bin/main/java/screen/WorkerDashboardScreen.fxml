                                    <Image url="@../../../../../../Downloads/icons8-notification-50.png" />
                                 </image>
                                 <HBox.margin>
                                    <Insets left="710.0" right="10.0" />
                                 </HBox.margin>
                              </ImageView>
                              <Button fx:id="logoutButton" mnemonicParsing="false" prefHeight="38.0" prefWidth="79.0" styleClass="logoutBtn" stylesheets="@../../../../../../Desktop/styleDashboard.css" text="Log out" textFill="WHITE">
